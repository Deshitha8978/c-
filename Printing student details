#include<stdio.h>
struct student
{
    int age;
    char name[20];
    float mark;
};
void main()
{
    struct student s[5];
    for(int i=0;i<5;i++)
    {
        printf("Enter details of student %d\n",i+1);
        scanf("%d",&s[i].age);
        scanf("%s",s[i].name);
        scanf("%f",&s[i].mark);
    }
    for(int i=0;i<5;i++)
    {
        printf("Age:%d ",s[i].age);
        printf("Name:%s ",s[i].name);
        printf("Marks:%f ",s[i].mark);
        printf("\n");
    }
    
}
